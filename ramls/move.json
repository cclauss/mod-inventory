{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Ids holder for moving items between holdings",
  "type": "object",
  "properties": {
    "to": {
      "description": "Id of the instance to which the holdings are moved, or id of the holding to which the items are moved.,
      "$ref": "uuid.json"
    },
    "ids": {
      "description": "Array of holding/item unique identifiers which will be moved to the new instance/holding",
      "type": "array",
      "items": {
        "$ref": "uuid.json"
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "to", "ids"
  ]
}
